{
    "name": "netresearch/magento-translation-manager",
    "description": "Manages the Magento 1.x, 2.x and x.x translation csv files ",
    "license": "MIT",
    "keywords": [
        "magento",
        "translation",
        "zf2"
    ],
    "homepage": "https://github.com/netresearch/magento-translation-manager",
    "config": {
        "platform": {
            "php" : "7.1"
        },
        "sort-packages": true
    },
    "require": {
        "zendframework/zend-cache": "^2.7",
        "zendframework/zend-component-installer": "^1.1",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-log": "^2.9",
        "zendframework/zend-mvc": "^3.1",
        "zendframework/zend-mvc-i18n": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    }
}
